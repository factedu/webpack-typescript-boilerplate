{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "JavaScript development environment by @lomboboo",
  "scripts": {
    "start": "cross-env NODE_ENV='dev' npm-run-all --parallel clean-build security-check webpack-dev-server",
    "webpack-dev-server": "webpack-dev-server --config=./config/webpack.dev.config.js",
    "security-check": "nsp check",
    "share": "lt --port 80 --subdomain lomboboo",
    "test": "cross-env NODE_ENV='test' karma start karma.conf.js",
    "test:single": "cross-env NODE_ENV='test' karma start karma.conf.js --single-run",
    "clean-build": "rimraf ./build",
    "prebuild": "npm-run-all clean-build security-check test:single",
    "build": "cross-env NODE_ENV='prod' webpack --progress --config=./config/webpack.prod.config.js"
  },
  "author": "Roman Prokopchuk",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "bootstrap": "^4.0.0-alpha.6",
    "chalk": "^2.3.0",
    "hard-source-webpack-plugin": "^0.4.9",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "node-object-hash": "^1.3.0",
    "popper.js": "^1.12.5"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/expect": "^1.20.1",
    "@types/jquery": "^2.0.40",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "@types/rx": "^4.1.1",
    "awesome-typescript-loader": "^3.0.8",
    "bootstrap-loader": "^2.1.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression": "1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "localtunnel": "1.8.1",
    "mocha": "^3.3.0",
    "nock": "8.1.0",
    "node-sass": "^4.5.3",
